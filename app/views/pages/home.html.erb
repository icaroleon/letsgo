
<!-- publishbox  -->
<div class="publish-box">
  <form>
    <div class="publish-box-input">
      <%= cl_image_tag current_user.photo.key %>
      <input class="ps-3 text-holder" type="text" placeholder="O que você está fazendo?" />
    </div>
    <button class="publish-box-publish-button">POSTAR</button>
  </form>
</div>


<!-- post 1 -->
<div class="post">
  <div class="post-avatar">
    <img
      src="https://i.pinimg.com/originals/a6/58/32/a65832155622ac173337874f02b218fb.png"
      alt=""
    />
  </div>

  <div class="post-body">
    <div class="post-header">
      <div class="post-header-text">
        <h3>
          Djez
          <span class="post-header-special"
            ><span class="material-icons post-badge"> verified </span>@djez</span
          >
        </h3>
      </div>
      <div class="post-header-description">
        <p>Lorem ipsum, dolor sit amet consectetur adipisicing elit.</p>
      </div>
    </div>
    <img
      src="https://www.focus2move.com/wp-content/uploads/2020/01/Tesla-Roadster-2020-1024-03.jpg"
      alt=""
    />
  </div>
</div>


